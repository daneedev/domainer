  "bugs": {
    "url": "https://github.com/domainerapp/domainer/issues"
  },
  "homepage": "https://github.com/domainerapp/domainer#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cloudflare": "^4.5.0",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^8.0.1",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sanitize-filename": "^1.6.3",
